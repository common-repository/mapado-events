{
    "name": "mapado/image-url-builder",
    "description": "Generate a full url from an image slug",
    "type": "library",
    "autoload": {
        "psr-4": { "Mapado\\ImageUrlBuilder\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Mapado\\ImageUrlBuilder\\Tests\\": "Tests/" }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Julien Deniau",
            "email": "julien.deniau@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.3",
        "atoum/config-extension": "^0.3.0",
        "mapado/php-cs-fixer-config": "^2.1",
        "phpstan/phpstan": "^0.10.1",
        "twig/twig": "^2.4"
    },
    "suggest": {
        "twig/twig": "For twig integration via the Mapado\\ImageUrlBuilder\\Twig\\UrlBuilderExtension extension"
    }
}
